<% if @videos.empty? then %>
  <h3>
    Sorry please try again
  </h3>
<% else %>
  <h3>
    <%=@videos.length%> videos found
  </h3>
  <ul id="results">
    <% @videos.each { |video| %>
      <%=render({:partial => "shared/remote_video", :locals => {:remote_video => video}})%>
    <% } %>
  </ul>
<% end %>
